    "markdown",
    "gatsby",
    "custom"
  ],
  "author": "Aayushi Agrawal aayushi.agrawal13@gmail.com",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.1.2",
    "cross-env": "^5.1.4",
    "unist-util-visit": "^2.0.1"
  }
}
