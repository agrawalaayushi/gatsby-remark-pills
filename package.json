{
  "name": "gatsby-remark-pills",
  "version": "1.0.3",
  "description": "Parse pill shortcode to pill html code in markdown",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
